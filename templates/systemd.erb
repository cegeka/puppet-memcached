## Managed by puppet
[Unit]
Description=Memcached instance<%= @instance_name %>
Before=httpd.service
After=network.target

[Service]
Type=simple
LimitNOFILE=65535
TimeoutStopSec=20
PermissionsStartOnly=true
RuntimeDirectory=memcached
RuntimeDirectoryMode=755
EnvironmentFile=-/etc/sysconfig/memcached<%= @instance_name %>
PIDFile=/run/memcached/memcached<%= @instance_name %>.pid
ExecStart=/bin/memcached -d -p $PORT -u $USER  -m $CACHESIZE -c $MAXCONN -P /run/memcached/memcached<%= @instance_name %>.pid $OPTIONS
[Install]
WantedBy=multi-user.target
